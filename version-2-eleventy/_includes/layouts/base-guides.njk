<!doctype html>
<html {% if section %} data-current="{{ section }}"{% endif %} lang="en">

	{% include "components/head.njk" %}

	<body class="fill">

		<div class="side">

			<header>
				{% include "components/skip.njk" %}
				{% include "components/nav-primary.njk" %}
				<div class="wrapper">
					<div class="fill__title fill__title__indiv">
					<h1>{{ title }}</h1>
					<p class="p-summary">{{ subtitle }}</p>
					</div>
				</div>
			</header>

		</div><!-- /side-->

		<main id="main">

			<div class="wrapper">
				<div class="panels">
				{{ layoutContent | safe }}
				</div>
			</div>

		</main>

		{% include "components/footer.njk" %}

	</body>

</html>
